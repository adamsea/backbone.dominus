{
	// Settings
	"passfail": false,
	"maxerr": 100,


	// Predefined globals whom JSHint will ignore.
	"browser": true,
	"jquery": true,


	// Custom globals.
	"predef": [
		"define",
		"module",
		"require",
		"sinon",
		"Backbone",
		"dominus",
		"reqwest",
		"QUnit"
	],


	// Development.
	"debug": false,
	"devel": false,


	// The Good Parts.
	"asi": false,
	"laxbreak": false,
	"bitwise": false,
	"boss": false,
	"curly": true,
	"eqeqeq": true,
	"eqnull": false,
	"evil": false,
	"expr": false,
	"forin": false,
	"immed": true,
	"latedef": true,
	"loopfunc": false,
	"noarg": true,
	"regexp": true,
	"regexdash": false,
	"scripturl": true,
	"shadow": false,
	"supernew": false,
	"undef": true,
	"unused": true,


	// Personal styling preferences.
	"newcap": true,
	"noempty": true,
	"nonew": true,
	"nomen": true,
	"onevar": true,
	"plusplus": true,
	"sub": false,
	"trailing": true,
	"white": false,
	"indent": 2
}
